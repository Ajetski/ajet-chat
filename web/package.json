{
	"name": "web",
	"version": "0.0.1",
	"scripts": {
		"svelte-kit": "node --experimental-specifier-resolution=node node_modules/\\@sveltejs/kit/dist/cli.js",
		"dev": "yarn svelte-kit dev",
		"build": "yarn svelte-kit build",
		"package": "yarn svelte-kit package",
		"preview": "yarn svelte-kit preview",
		"prepare": "yarn svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@apollo/client": "^3.5.10",
		"socket.io-client": "^4.5.1",
		"svelte-apollo": "^0.5.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.76",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module"
}
